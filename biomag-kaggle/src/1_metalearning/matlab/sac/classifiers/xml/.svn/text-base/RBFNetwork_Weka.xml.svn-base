<?xml version="1.0" encoding="UTF-8" ?>
<!-- this is a comment -->
<optionList>  
  <option>
    <classifier>RBFNetwork_Weka</classifier>
    <description>clustering seed</description>
    <type>intList</type>
    <str>-S %d</str>
    <values>0:100000</values>
    <default>1</default>
    <priority>-1</priority>
  </option>
  <option>
    <classifier>RBFNetwork_Weka</classifier>
    <description>maximum iterations for logistic regression</description>
    <type>intList</type>
    <str>-M %d</str>
    <values>-1 10 100</values>
    <default>-1</default>
    <priority>.5</priority>
  </option>
  <option>
    <classifier>RBFNetwork_Weka</classifier>
    <description>minimum standard deviation for the clusters</description>
    <type>exprRange</type>
    <str>-W %0.5g</str>
    <expr>2.^n</expr>
    <values>-5 10</values>
    <default>-3.3219</default>
    <priority>1</priority>
  </option>
   <option>
    <classifier>RBFNetwork_Weka</classifier>
    <description>ridge value</description>
    <type>exprRange</type>
    <str>-R %0.5g</str>
    <expr>2.^n</expr>
    <values>-30 -1</values>
    <default>-26.5754</default>
    <priority>1</priority>
  </option>
</optionList>
